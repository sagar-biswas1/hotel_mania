{
    "version": 2,
    "builds": [
      {
        "src": "dist/src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/src/index.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      }
    ],
    "project": "api-gateway-hotel-mania"
  }
  